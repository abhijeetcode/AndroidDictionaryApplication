<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="inducesmile.com.androiddictionaryapplication">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">


        <activity
            android:name=".NavigationDrawerLayout"
            android:label="@string/title_activity_navigation_drawer_layout"
            android:theme="@style/AppTheme.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".DictionaryActivity"
            android:label="@string/title_activity_dictionary"></activity>


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">

        </activity>



    </application>

</manifest>